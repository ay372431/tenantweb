    "svgo-loader": "^3.0.0",
    "vue-template-compiler": "^2.6.11",
    "vue-wxlogin": "^1.0.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "prefer-const": "off",
      "generator-star-spacing": "off",
      "no-console": 0,
      "no-inline-comments": 0,
      "indent": [
        2
      ],
      "semi": [
        2,
        "always"
      ],
      "no-mixed-spaces-and-tabs": [
        0,
        false
      ],
      "space-before-function-paren": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
